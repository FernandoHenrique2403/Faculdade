algoritmo (exercicio_5)
	real a,b,c,delta,x,x1,x2 
	
	inicio
	escreva ("digite aqui o valor de a");
	leia(a);
	escreva ("digite aqui o valor de b");
	leia(b);
	escreva ("digite aqui o valor de c");
	leia(c);
	delta = (b*b) -4*a*c
	x =(-b+-raiz(delta))/2*a
	x1 = (-b+raiz(delta))/2*a
	x2 = (-12-raiz(delta))/2*a
	
	escreva ("A raiz positiva é; " + x1 + "e a raiz negativa é: "+ x2);
	
	fim
	
	

	
	