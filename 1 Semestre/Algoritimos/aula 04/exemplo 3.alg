algoritmo nota
    real nota1, nota2,media
	
    inicio
        escreva ("digite a nota 1")
        leia(nota1 )
		escreva ("digite a nota 2")
        leia(nota2 )
		media = (nota1+nota2)/2
        se (media>=6.0){
            escreva("passou de ano")
			}
        senao   {
           escreva("reprovado")
		   }
		   
   fim
   